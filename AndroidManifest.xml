<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="auto" package="yglo.apps.instasavegram" platformBuildVersionCode="22" platformBuildVersionName="3.05-1819727">
